Adafruit_NeoPixel strip = Adafruit_NeoPixel(256*BoadX*BoadY, PIN, NEO_GRB + NEO_KHZ800);

void setup(){
  strip.begin();
  strip.show();// Initialize all pixels to 'off'
}
void loop(){
  Scroll();
  strip.show();
  delay(1000/FPS);
}
//スクロール中の位置
int posy = (Blank)?-16*BoadX:0;
void Scroll(){
  for(int i=0;i<16*BoadX;i++){
    for(int k=0;k<16*BoadY;k++){
      if((i+posy < 0 || i+posy >= RowS) && Blank){
        SetPixcel(i,k,0,0,0);
      }
      else{
        SetPixcel(i,k,pgm_read_byte_near(&dotChar[i+posy][k][0]),pgm_read_byte_near(&dotChar[i+posy][k][1]),pgm_read_byte_near(&dotChar[i+posy][k][2]));
      }
    }
  }
  posy += ScrollNum;
  if(posy >= RowS || (posy+16*BoadX>=RowS && !Blank)){
    posy = (Blank)?-16*BoadX:0;
  }
}

//xy座標で色を設定する
void SetPixcel(int x,int y,int r,int g,int b){
  strip.setPixelColor((y%16)*16+((y%2)?(15-x%16):x%16)+(x/16)*256+(y/16)*BoadX*256,r,g,b);
}


